<!--                         <i class="la la-remove"></i>-->
<!--                     </a>-->
                </td>
            </tr>
            <%});%>
            </tbody>
            <tfoot>
            <tr>
                <th>ID</th>
                <th>img</th>
                <th>link</th>
                <th>status</th>
                <th>содержание</th>

                <th>Действия</th>
