<section class="bg-body section-space-default">
    <div class="container">
        <div class="row tab-space1 mb-25 ">
            <div class="col-12">
                <div class="topic-border color-apple mb-30 width-100">
                    <div class="topic-box-lg color-persian-green ">
                        <a class="color-link" href="/cat/<%=menu[0].id%>/<%=lang%>"><%=menu[0][`title_${lang}`]%></a>
                    </div>
                </div>
            </div>



            <% result.forEach(post=>{ %>
                <% if(post.cat_id=='1'){%>
                <div class="col-lg-3 col-md-6 col-sm-12 col-6 ">
                    <div class="img-overlay-70 img-scale-animate mb-2">
                        <div class="mask-content-xs">
                            <div class="post-date-light">
                                <ul>
                                    <li>
                                                        <span>
                                                            <i class="fa fa-calendar" aria-hidden="true"></i>
                                                        </span>
                                        <span class="dateTime" value="<%=post.date%>"></span>

                                    </li>
                                </ul>
                            </div>
                            <h3 class="title-medium-light">
                                <a href="/post/<%=post.id%>/<%=lang%>"><%=post.title%> </a>
                            </h3>
                        </div>

                        <img src="/upload/temp/560/<%=post.featured_image.image_path%>" alt="<%=post.featured_image.image_alt%>"  class="img-fluid width-100">
