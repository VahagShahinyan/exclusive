                                        <img src="<%=bannerJson["1"].img_src%>" alt="ad" class="img-fluid">
                                    </a>
                    <%}else if(bannerJson["1"].status=='html' && bannerJson["1"].html ){%>
                    <%-bannerJson["1"].html -%>
                        <%}%>
                    <%}%>
                </div>
            </div>
        </div>
        <%}%>
    </div>
</section>
