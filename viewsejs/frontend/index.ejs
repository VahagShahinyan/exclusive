<%-include('../includes/header.ejs')%>
<section class="container  section-space-less2" style="    margin-top: 1px;  ">
    <div class="">
        <div class="section-space-bottom-less4" style="padding: 0 0 10px 0;">
            <div class="row tab-space2 ">
                <div class="col-lg-5 col-md-12">
                    <div class="img-overlay-70 img-scale-animate mb-4 political-main slide1">
                        <% var politicalCount=0 %>
                        <% result.forEach(post=>{%>
                            <% if(post.cat_id=='2' && politicalCount ==0){%>
                            <img src="/upload/temp/800/<%=post.featured_image.image_path%>"    alt="<%post.featured_image.image_alt%>"  class="img-fluid width-100">
                            <div class="mask-content-lg">
                                <div class="topic-box-sm color-cod-gray mb-20"><%=post.cat_title%></div>
                                <div class="post-date-light">
                                    <ul>
                                        <li>
                                            <span><i class="fa fa-calendar" aria-hidden="true"></i></span>
                                            <span class="dateTime" value="<%=post.date%>"></span>


                                        </li>
                                    </ul>
                                </div>
                                <h1 class="title-medium-light">
                                    <a href="/post/<%=post.id%>/<%=lang%>"><%=post.title%></a>
                                </h1>
                            </div>
                            <% politicalCount=1 %>
                            <%}%>

                        <%})%>
                    </div>
                </div>
                <div class="col-lg-7 col-md-12">
                    <div class="row tab-space2 political-secondary ">
                        <!--qaghakan 2-->
                        <% var politicalSecondCount=0 %>
                        <% result.forEach(post=>{ %>
                            <% if(post.cat_id=='2' && politicalSecondCount ==1){%>
                            <div class="col-sm-6 col-12">
                                <div class="img-overlay-70 img-scale-animate mb-4">
                                    <div class="mask-content-sm">
                                        <div class="topic-box-sm color-cod-gray mb-10"><%=post.cat_title%></div>
                                        <h3 class="title-medium-light">
                                            <a href="/post/<%=post.id%>/<%=lang%>"><%=post.title%> </a>
                                        </h3>
                                    </div>

                                    <img src="/upload/temp/560/<%=post.featured_image.image_path%>" alt="<%=post.featured_image.image_alt%>" class="img-fluid width-100">
                                </div>
                            </div>
                            <%}else if(post.cat_id=='2' && politicalSecondCount ==0){%>
                                <% politicalSecondCount=1   %>
                            <%}%>
                        <%})%>
                        <!--qaghakan 2-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
